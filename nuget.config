<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="MyNuGetRegistry" value="%MY_NUGET_REGISTRY%" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="MyNuGetRegistry">
      <package pattern="ChanIvan.*" />
    </packageSource>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>